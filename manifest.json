{
    "name": "Wavenet reader",
    "description": "Wavenet voice chrome page reader",
    "version": "1.3",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ],
    "background": {
        "scripts": [
            "wavenet-server.js",
            "background.js"
        ]
    },
    "icons": {
        "256": "/images/icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{1b3785b8-6999-4c56-8ffb-8e79e18e03a3}"
        }
    }
}